{
  "name": "@lamnhan/seminjecto",
  "version": "1.0.0-beta2",
  "description": "Simple dependency injection for Typescript modules.",
  "author": "Lam Nhan",
  "homepage": "https://lamnhan.com",
  "license": "MIT",
  "main": "./src/public-api.js",
  "typings": "./src/public-api.d.ts",
  "bin": {
    "semidi": "./src/bin.js"
  },
  "files": [
    "src",
    "!**/*.ts",
    "**/*.d.ts"
  ],
  "scripts": {
    "compile": "tsc -p .",
    "fix": "gts fix",
    "build": "npm run compile && npm i -g",
    "docs": "ayedocs generate",
    "test": "npm run compile && nyc --cache false mocha test/**/*.js",
    "coverage": "nyc --cache false report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "node",
    "module",
    "dependency-injection"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lamnhan/seminjecto.git"
  },
  "bugs": {
    "url": "https://github.com/lamnhan/seminjecto/issues"
  },
  "dependencies": {
    "adm-zip": "^0.4.13",
    "axios": "^0.19.1",
    "chalk": "^3.0.0",
    "commander": "^4.1.0",
    "fs-extra": "^8.1.0",
    "tty-table": "^2.8.5"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.4.32",
    "@types/chai": "^4.2.7",
    "@types/fs-extra": "^8.0.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.1.8",
    "chai": "^4.2.0",
    "coveralls": "^3.0.9",
    "gts": "^1.1.2",
    "mocha": "^7.0.0",
    "nyc": "^15.0.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.5"
  }
}

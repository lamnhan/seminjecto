{
  "name": "@lamnhan/seminjecto",
  "version": "1.0.0-beta5",
  "description": "Simple dependency injection for Typescript modules.",
  "author": "Lam Nhan",
  "homepage": "https://lamnhan.com",
  "license": "MIT",
  "main": "src/public-api.js",
  "types": "src/public-api.d.ts",
  "bin": {
    "semidi": "src/bin.js"
  },
  "files": [
    "src",
    "!**/*.ts",
    "**/*.d.ts"
  ],
  "scripts": {
    "compile": "tsc",
    "build": "npm.cmd run compile && npm.cmd i -g",
    "docs": "ayedocs generate",
    "test": "npm.cmd run compile && nyc --cache false mocha test/**/*.js",
    "coverage": "nyc --cache false report --reporter=text-lcov | coveralls",
    "check": "gts check",
    "fix": "gts fix"
  },
  "keywords": [
    "node",
    "dependency-injection",
    "module",
    "cli",
    "app"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lamnhan/seminjecto.git"
  },
  "bugs": {
    "url": "https://github.com/lamnhan/seminjecto/issues"
  },
  "dependencies": {
    "adm-zip": "^0.4.16",
    "axios": "^0.20.0",
    "chalk": "^4.1.0",
    "commander": "^6.1.0",
    "fs-extra": "^9.0.1",
    "tty-table": "^4.1.3"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.4.33",
    "@types/chai": "^4.2.12",
    "@types/fs-extra": "^9.0.1",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.10.1",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "gts": "^2.0.2",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "typescript": "^3.9.3"
  }
}
